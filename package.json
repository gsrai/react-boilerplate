{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "My custom react boilerplate, less time setting up and more time developing.",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker-build": "docker build -t app-ui .",
    "docker-start": "docker run -it -p 8080:8080 --name app-ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsrai/react-boilerplate.git"
  },
  "keywords": [],
  "author": "Gagondeep Srai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gsrai/react-boilerplate/issues"
  },
  "homepage": "https://github.com/gsrai/react-boilerplate#readme",
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ]
  },
  "eslintIgnore": [
    "dist"
  ],
  "dependencies": {
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
